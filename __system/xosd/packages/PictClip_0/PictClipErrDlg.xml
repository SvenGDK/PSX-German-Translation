<!-- 警告 = 後戻りできる /-->
<!-- エラー = 終了する /-->

<!-- アルバム内に写真がない警告 /-->
<Screen name="FileNotFoundInAlbumError" visible="0" z_depth="-1">
	<Text name="text0" str="Für Pict Story sind keine Fotos verfügbar."				y="-6" size="16" alignment="center" />
	<Text name="text1" str="Bitte wählen Sie ein anderes Album aus.　　　　"				y="16" size="16" alignment="center" />
</Screen>

<!-- Pict Storyファイルが壊れている警告 /-->
<Screen name="PictClipFileBrokenError" visible="0" z_depth="-1">
	<Text name="text0" str="Diese Pict Story kann nicht geändert werden.　　"					y="-6" size="16" alignment="center" />
	<Text name="text1" str="Bitte wählen Sie eine andere Pict Story aus."					y="16" size="16" alignment="center" />
</Screen>

<!-- 入力した値と同名、もしくは無記名の警告 /-->
<Screen name="TitleInSameOrNoneError" visible="0" z_depth="-1">
	<Text name="text0" str="Der eingegebene Name ist ungültig oder wird bereits verwendet."			y="-6" size="16" alignment="center" />
	<Text name="text1" str="Bitte ändern Sie den Namen.　　　　　　　　　　"			y="16" size="16" alignment="center" />
</Screen>

